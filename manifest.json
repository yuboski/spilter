{
    "manifest_version": 3,
   
    "name": "SPILTER - Scheduled Pipelines filter",
    "version": "1.0.0",
    "description": "Sorts the table of scheduled pipelines in Gitlab",
   
    "content_scripts": [{
      "js": ["content.js"],
      "matches": [
        "https://*/*/-/pipeline_schedules?*",
        "https://*/*/-/pipeline_schedules"
      ]
    }],
    "icons" : {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
